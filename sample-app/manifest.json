{
  "name": "Chrome Options",
  "version": "0.2",
  "manifest_version": 2,
  "browser_action": {
    "default_icon": {
      "19": "icons/icon19.png",
      "38": "icons/icon38.png"
    },
    "default_title": "Chrome Options Sample App"
  },
  "icons": {
    "19": "icons/icon19.png",
    "38": "icons/icon38.png",
    "128": "icons/icon128.png"
  },
  "permissions": [
    "storage"
  ],
  "options_page": "node_modules/angular-chrome-options/dist/index.html",
  "web_accessible_resources": ["icons/icon38.png"]
}
